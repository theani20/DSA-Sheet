#include <bits/stdc++.h> 
vector<int> rotateArray(vector<int>& arr, int n) 
{

 int f=arr[0];
 
 for(int i=0;i<n-1;i++)
 {
     arr[i]=arr[i+1];
 }
 arr[n-1]=f;
 return arr;
}
//////////////////////////////////

#include <bits/stdc++.h> 
vector<int> rotateArray(vector<int>& arr, int n) 
{
vector<int> a(n);
 for(int i=0;i<n-1;i++)
 {
     a[i]=arr[i+1];
 }
 a[n-1]=arr[0];
 return a;
}
